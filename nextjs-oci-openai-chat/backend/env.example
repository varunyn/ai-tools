# OCI Configuration (used by Gen AI client)
OCI_REGION=us-chicago-1
OCI_COMPARTMENT_ID=your-compartment-id-here
# Optional: OCI_CONFIG_FILE=oci-config  OCI_CONFIG_PROFILE=CHICAGO (defaults used if not set)
# System prompt: not used by this backend. Prompts and system messages come from the client only (Open WebUI or any other). Configure system prompt in your client (e.g. Open WebUI chat/model settings).
# Gen AI base endpoint (optional). Use same var as embeddings. Without /actions/v1.
# App uses: base + /actions/v1 for chat.completions; base only for conversations and responses.
# OCI_GENERATIVE_AI_ENDPOINT=https://inference.generativeai.us-chicago-1.oci.oraclecloud.com

# Model configuration
MODEL_ID=meta.llama-3.1-70b-instruct
MAX_TOKENS=1000
TEMPERATURE=0.7

# Oracle 23ai / RAG tools â€” set these only if you use the search_knowledge_base tool.
# The backend only forwards tool calls; your client (e.g. run_rag_tool_client) needs DB access to run the tool.
# For Oracle Cloud (Wallet-based connection):
# ORACLE_USER=your-oracle-user
# ORACLE_PASSWORD=your-password
# ORACLE_DSN=your_dsn_high
# ORACLE_CONFIG_DIR=/path/to/Wallet_yourdb
# ORACLE_WALLET_LOCATION=/path/to/Wallet_yourdb
# ORACLE_WALLET_PASSWORD=your-wallet-password

# Oracle Table Configuration
# Table name: {org}_web_embeddings (e.g. acme_web_embeddings). Must have CONTENT, VEC, SOURCE_URL.
ORACLE_TABLE_NAME=web_embeddings

# RAG MCP server (tools/oracle/mcp_server.py --transport sse): port for SSE/HTTP (default 3021)
# RAG_MCP_PORT=3021

# Embedding Configuration
# Options: 'oracle' (Oracle 23ai native) or 'oci' (Cohere via OCI)
EMBEDDING_METHOD=oracle

# Oracle 23ai Native Embedding (Recommended)
# This is the name of your ONNX embedding model stored in Oracle AI Database
# You must load the model into Oracle first (see Oracle documentation)
ORACLE_EMBEDDING_MODEL=doc_model

# Oracle Vector Type (FLOAT32 or FLOAT64)
# Should match your table schema: VECTOR(n, FLOAT32) or VECTOR(n, FLOAT64)
ORACLE_VECTOR_TYPE=FLOAT32

# Oracle Cloud Wallet Configuration (Optional - for Oracle Cloud connections)
# ORACLE_CONFIG_DIR=/path/to/config
# ORACLE_WALLET_LOCATION=/path/to/wallet
# ORACLE_WALLET_PASSWORD=your-wallet-password

# Alternative: OCI Generative AI Embeddings (cohere.embed-v4.0)
# EMBEDDING_METHOD=oci
# OCI_EMBEDDING_MODEL=cohere.embed-v4.0
# OCI_GENERATIVE_AI_ENDPOINT=https://inference.generativeai.us-chicago-1.oci.oraclecloud.com
# OCI_CONFIG_PROFILE=DEFAULT
# OCI_CONFIG_PATH=~/.oci/config
